{
    "name": "kiboit/phast",
    "description": "A toolbox for optimizing web page performance",
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kiboit/jsmin-php"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "nikic/php-parser": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "facebook/webdriver": "^1.5",
        "ext-dom": "*",
        "friendsofphp/php-cs-fixer": "^2.16",
        "kiboit/jsmin-php": "dev-control-chars"
    },
    "autoload": {
        "psr-4": {
            "Kibo\\Phast\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kibo\\Phast\\": ["test/php/"]
        }
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    }
}
